{
  "plugins": [
    ["react-hot-loader/babel"],
	["@babel/plugin-proposal-class-properties"],
    [
      "import",
      {
        libraryName: "antd-mobile",
        style: "css"
      }
    ]
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "edge": 17,
          "firefox": 60,
          "chrome": 67,
          "safari": 11.1
        },
        "useBuiltIns": "usage"
      }
    ],
    "@babel/preset-react"
  ]
}

